{
    "name": "ottaviano/faker-gravatar",
    "license": "MIT",
    "description": "Faker Gravatar image provider",
    "type": "library",
    "keywords": [
        "faker",
        "fixtures",
        "data",
        "gravatar",
        "image",
        "avatar"
    ],
    "authors": [
        {
            "name": "Dimitri Gritsajuk",
            "email": "gritsajuk.dimitri@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "fakerphp/faker": "^1.20",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Ottaviano\\Faker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ottaviano\\Faker\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
